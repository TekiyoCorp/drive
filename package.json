{
  "name": "nextjs-drive",
  "version": "0.1.0",
  "private": true,
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions",
      "not dead",
      "not ie 11",
      "not op_mini all",
      "not dead"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "dev": "next start",
    "build": "next build",
    "build:optimized": "./scripts/build-optimized.sh",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "eslint",
    "optimize:images": "./scripts/optimize-images-performance.sh",
    "analyze": "ANALYZE=true npm run build",
    "performance:test": "lighthouse http://localhost:3000 --output=json --output=html --output-path=./performance-report",
    "performance:production": "./scripts/test-production.sh",
    "test:lighthouse": "lighthouse http://localhost:3000 --only-categories=performance --output=html --output-path=./lighthouse-performance.html",
    "bundle:analyze": "pnpm build:analyze && npx serve out"
  },
  "dependencies": {
    "@liquidglass/react": "^0.1.3",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.12",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-intersection-observer": "^9.16.0",
    "react-leaflet": "^5.0.0",
    "react-leaflet-cluster": "^3.1.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.3",
    "@tailwindcss/postcss": "^4",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "critters": "^0.0.25",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "sharp": "^0.34.4",
    "tailwindcss": "^4",
    "terser": "^5.36.0",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5"
  }
}
