{
  "kind": "collectionType",
  "collectionName": "vehicles",
  "info": {
    "singularName": "vehicle",
    "pluralName": "vehicles",
    "displayName": "Vehicle",
    "description": "Vehicle listings for the catalog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "subtitle": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "string",
      "required": true
    },
    "km": {
      "type": "string",
      "required": true
    },
    "year": {
      "type": "string",
      "required": true
    },
    "fuel": {
      "type": "string",
      "required": true
    },
    "trans": {
      "type": "string",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": ["images"]
    },
    "description": {
      "type": "text",
      "required": false
    },
    "features": {
      "type": "json",
      "required": false
    },
    "specifications": {
      "type": "json",
      "required": false
    },
    "status": {
      "type": "enumeration",
      "enum": ["available", "sold", "reserved"],
      "default": "available"
    },
    "featured": {
      "type": "boolean",
      "default": false
    },
    "order": {
      "type": "integer",
      "default": 0
    }
  }
}
